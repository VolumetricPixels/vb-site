#homepage
  background-position: center bottom
  padding: 120 0 220
  background-color: #13A7C7

*
  margin: 0

html, body
  height: 100%

#content
  padding-top: 20px

#wrapper
  min-height: 100%
  height: auto !important
  height: 100%
  margin: 0 auto -60px

#footer, .push
  height: 60px

#footer
  background-color: #f5f5f5

#footer .container
  padding-top: 20px

@media (max-width: 767px)
  #footer
    margin-left: -20px
    margin-right: -20px
    padding-left: 20px
    padding-right: 20px
  .small-gone
    display: none
